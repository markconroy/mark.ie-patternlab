<div class="sg-header-styles layout-contained padding-horizontal margin-top">
  {% for i in 1..6 %}
    <div class="sg-pattern pl-heading-examples">
      <h3 class="sg-pattern-title">h{{ i }}</h3>
      <h{{i}} class="h{{ i }}">{{ title|replace({'Title': 'H' ~ i ~ ' Title example' }) }}</h{{i}}>
    </div>
  {% endfor %}
</div>
{% if meta %}
  <p>
    <small>
      {{ meta.description }}</small>
  </p>
{% endif %}
